<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="space_ship_list_action" model="ir.actions.act_window">
        <field name="name">Space Ship</field>
        <field name="res_model">space.ship</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new Space Ship
            </p>
        </field>
    </record>

    <menuitem
        id="space_mission_menu"
        name="Space Mission"
        sequence="1"/>

    <menuitem
        id="space_mission_submenu"
        name="Space Mission"
        parent="space_mission_menu"
        sequence="2"/>

    <menuitem
        id="space_ship_menu"
        name="Space Ships"
        action="space_ship_list_action"
        parent="space_mission_submenu"
        sequence="3"/>

</odoo>
